{
    "name": "@nexovision/ui-kit",
    "version": "0.0.1",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "license": "MIT",
    "scripts": {
        "build": "tsup src/index.ts --format esm --dts --clean --external react,react-native",
        "dev": "tsup src/index.ts --format esm --dts --watch --external react,react-native",
        "lint": "eslint ."
    },
    "peerDependencies": {
        "react": "*"
    },
    "devDependencies": {
        "@types/react": "^18.0.0",
        "@types/react-native": "^0.70.0",
        "tailwindcss": "^3.4.1",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@float.js/lite": "workspace:*",
        "nativewind": "^4.2.1",
        "react-native-web": "^0.21.2"
    }
}