{
    "name": "@float.js/native",
    "version": "1.0.0",
    "description": "React Native primitives for Float.js - single source of truth",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rm -rf dist"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-native": "^0.83.1"
    },
    "devDependencies": {
        "@types/react": "^18.3.18",
        "react": "^18.3.1",
        "react-native": "^0.83.1",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3"
    },
    "keywords": [
        "float.js",
        "react-native",
        "primitives",
        "ui",
        "components"
    ],
    "author": "Float.js Team",
    "license": "MIT"
}